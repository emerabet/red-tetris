{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.(ts|tsx|js)$",
    "coverageReporters": [
      "text",
      "html",
      "json"
    ]
  },
  "scripts": {
    "test": "PORT=4000 jest",
    "coverage": "PORT=4000 jest --coverage",
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue, green\" \"tsc --watch\" \"PORT=4000 nodemon --inspect dist/index.js\"",
    "tsc": "tsc",
    "prod": "PORT=4000 node dist/index.js",
    "lint": "tslint --project tslint.json 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.18",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.9",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/socket.io": "^2.1.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "socket.io": "^2.2.0"
  }
}
