{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [{
            "name": "client",
            "image": "emerabet/red-tetris-frontend",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "back",
            "image": "emerabet/red-tetris-backend",
            "hostname": "back",
            "essential": false
        },
        {
            "name": "proxy",
            "image": "emerabet/red-tetris-proxy",
            "hostname": "proxy",
            "essential": true,
            "portMappings": [{
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "back"]
        }
    ]
}