    "enzyme-adapter-react-16": "^1.12.1",
    "react-testing-library": "^6.1.2",
    "redux-testkit": "^1.0.6",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.2"
